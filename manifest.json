{
  "manifest_version": 2,
  "name": "enkel",
  "description": "提供JSON格式化、Base64加密解密、传图识色、文字加密等开发工具",
  "version": "1.0.6",
  "permissions": [
    "activeTab",
    "tabs",
    "declarativeContent",
    "file:///*",
    "https://secure.flickr.com/",
    "https://talkapi.dei2.com/",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "html/index.html/#/popup"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/jquery-1.11.1.min.js", "js/jquery.json.js", "js/json2.js", "js/jsonlint.js", "content-script.js"],
    "css": [],
    "run_at": "document_start"
  }],
  "background": {
    "page": "html/index.html#/scripts"
  },
  "chrome_url_overrides": {
    "newtab": "html/index.html#/blank"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}